<%= provide(:title, "Orgao Publico") %>
<p>

<header class="codrops-header">
  <h1 style="padding-top: 30px;"><%= link_to @superior_public_agency.name.downcase.split.map(&:capitalize).join(' '), superior_path(@superior_public_agency) %><span id="surprise"><%= @public_agency.name.downcase.capitalize %></span></h1>
</header>

<script type="text/javascript">
var WAVES = ['waves-red','waves-yellow','waves-orange','waves-purple','waves-green','waves-teal']
  $('#surprise').dblclick(function(){
      $('#haha').show();
      setTimeout(function(){
        $('#haha').hide();
      },6000);
  });
  var count =0;
  function haha(){    
    $('#haha').removeClass(WAVES[count++%7]);
    $('#haha').addClass(WAVES[count%7]);
  }
</script>

<div class="container">
  <button id='haha' class='btn waves-effect' onclick="haha()" style='background: white; color:black; width:200px; height: 50px; display: none;'>Surpreenda-me</button>
</div>
  
<div id="contencao_outros">
    <ul class="collapsible popout" data-collapsible="accordion">
      <li>
        <div class='collapsible-header active' style='background:#FF8F2F;' ><i class="material-icons">money_off</i>Gastos</div>
