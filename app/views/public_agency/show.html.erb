<script type="text/javascript">
var WAVES = ['waves-red','waves-yellow','waves-orange','waves-purple','waves-green','waves-teal']
var count =0;
  function haha(){    
    $('#haha').removeClass(WAVES[count++%7]);
    $('#haha').addClass(WAVES[count%7]);
  }
</script>

